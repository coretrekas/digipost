{
    "name": "coretrekas/digipost",
    "description": "PHP client library for the Digipost API - Send digital mail to Norwegian citizens and businesses",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "digipost",
        "digital-mail",
        "norway",
        "api-client",
        "postal",
        "documents"
    ],
    "authors": [
        {
            "name": "Coretrek AS",
            "email": "post@coretrek.no"
        }
    ],
    "homepage": "https://github.com/coretrekas/digipost",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0",
        "ramsey/uuid": "^4.7",
        "spatie/data-transfer-object": "^3.9",
        "nesbot/carbon": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "rector/rector": "^2.0",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6",
        "php-http/mock-client": "^1.6",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Coretrek\\Digipost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coretrek\\Digipost\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test:coverage": "pest --coverage",
        "analyse": "phpstan analyse",
        "format": "pint",
        "format:check": "pint --test",
        "rector": "rector process",
        "rector:dry": "rector process --dry-run",
        "quality": [
            "@format:check",
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
